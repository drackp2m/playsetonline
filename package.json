{
	"name": "set-online",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"ng": "nx",
		"postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
		"start": "nx run-many --target serve --all",
		"start:api": "nx run-many --target serve --projects api",
		"start:app": "nx run-many --target serve --projects app",
		"build": "nx build",
		"test": "nx run-many --target=test --all --parallel",
		"test:affected": "nx affected --target=test --base=dev",
		"test:api": "nx test api",
		"test:app": "nx test app",
		"migration:check": "mikro-orm migration:pending",
		"migration:create": "mikro-orm migration:create",
		"migration:execute": "mikro-orm migration:up",
		"prettier": "prettier -c --loglevel warn",
		"prettier:fix": "prettier -w --loglevel warn",
		"lint": "eslint -f tools/eslint-formatter --max-warnings 0",
		"lint:fix": "yarn lint --fix",
		"stylelint": "stylelint --max-warnings 0",
		"stylelint:fix": "yarn stylelint --fix",
		"prepare": "husky install"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~14.2.10",
