{
	"name": "set-online",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"ng": "nx",
		"postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
		"start": "nx run-many --target serve --all",
		"start:api": "nx run-many --target serve --projects api",
		"start:app": "nx run-many --target serve --projects app",
		"build": "nx build",
		"test": "nx run-many --target=test --all --parallel",
		"test:affected": "nx affected --target=test --base=dev",
		"test:api": "nx test api",
		"test:app": "nx test app",
		"migration:check": "mikro-orm migration:pending",
		"migration:create": "mikro-orm migration:create",
		"migration:execute": "mikro-orm migration:up",
		"prettier": "prettier -c --loglevel warn",
		"prettier:fix": "prettier -w --loglevel warn",
		"lint": "eslint -f tools/eslint-formatter --max-warnings 0",
		"lint:fix": "yarn lint --fix",
		"stylelint": "stylelint --max-warnings 0",
		"stylelint:fix": "yarn stylelint --fix",
		"prepare": "husky install"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~14.2.3",
		"@angular/common": "~14.2.3",
		"@angular/compiler": "~14.2.3",
		"@angular/core": "~14.2.3",
		"@angular/forms": "~14.2.3",
		"@angular/platform-browser": "~14.2.3",
		"@angular/platform-browser-dynamic": "~14.2.3",
		"@angular/router": "~14.2.3",
		"@mikro-orm/cli": "^5.4.2",
		"@mikro-orm/core": "^5.4.2",
		"@mikro-orm/migrations": "^5.4.2",
		"@mikro-orm/nestjs": "^5.1.2",
		"@mikro-orm/postgresql": "^5.4.2",
		"@mikro-orm/seeder": "^5.4.2",
		"@nestjs/apollo": "^10.1.0",
		"@nestjs/common": "^9.1.2",
		"@nestjs/config": "^2.2.0",
		"@nestjs/core": "^9.1.2",
		"@nestjs/graphql": "^10.1.2",
		"@nestjs/jwt": "^9.0.0",
		"@nestjs/passport": "^9.0.0",
		"@nestjs/platform-express": "^9.1.2",
		"@nestjs/throttler": "^3.0.0",
		"@nrwl/angular": "14.7.11",
		"apollo-server-express": "^3.10.2",
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"graphql": "^16.6.0",
		"graphql-subscriptions": "^2.0.0",
		"joi": "^17.6.1",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "~7.5.0",
		"ts-morph": "^16.0.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~14.2.3",
		"@angular-eslint/eslint-plugin": "~14.1.2",
		"@angular-eslint/eslint-plugin-template": "~14.1.2",
		"@angular-eslint/template-parser": "~14.1.2",
		"@angular/cli": "~14.2.3",
		"@angular/compiler-cli": "~14.2.3",
		"@angular/language-service": "~14.2.3",
		"@nestjs/cli": "^9.1.3",
		"@nestjs/schematics": "^9.0.3",
		"@nestjs/testing": "^9.1.2",
		"@nrwl/cli": "14.7.11",
		"@nrwl/cypress": "14.7.11",
		"@nrwl/eslint-plugin-nx": "14.7.11",
		"@nrwl/jest": "14.7.11",
		"@nrwl/linter": "14.7.11",
		"@nrwl/nest": "14.7.11",
		"@nrwl/node": "14.7.11",
		"@nrwl/workspace": "14.7.11",
		"@types/bcryptjs": "^2.4.2",
		"@types/jest": "^29.0.3",
		"@types/node": "^18.7.18",
		"@types/passport-jwt": "^3.0.6",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"cypress": "^10.8.0",
		"eslint": "~8.23.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.5.1",
		"eslint-plugin-cypress": "^2.10.3",
		"eslint-plugin-import": "^2.26.0",
