		"ts-node": "^10.9.1",
		"typescript": "~4.8.3"
	},
	"mikro-orm": {
		"useTsNode": true,
		"tsConfigPath": "./apps/api/tsconfig.app.json",
		"configPaths": [
			"./apps/api/src/common/config/mikro-orm.config.ts"
		]
	}
}
