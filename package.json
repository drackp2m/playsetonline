{
	"name": "set-online",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"ng": "nx",
		"postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
		"start": "nx run-many --target serve --all",
		"start:api": "nx run-many --target serve --projects api",
		"start:app": "nx run-many --target serve --projects app",
		"build": "nx build",
		"test": "nx run-many --all --target=test --parallel",
		"test:api": "nx test api",
		"test:app": "nx test app",
		"migration:create": "mikro-orm migration:create",
		"migration:pending": "mikro-orm migration:pending",
		"migration:up": "mikro-orm migration:up",
		"prettier": "prettier -c",
		"prettier:fix": "prettier -w --loglevel warn",
		"lint": "eslint -f tools/eslint-formatter --max-warnings 0",
		"lint:fix": "yarn lint --fix",
		"stylelint": "stylelint --max-warnings 0",
		"stylelint:fix": "yarn stylelint --fix",
		"prepare": "husky install"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~14.1.1",
		"@angular/common": "~14.1.1",
		"@angular/compiler": "~14.1.1",
		"@angular/core": "~14.1.1",
		"@angular/forms": "~14.1.1",
		"@angular/platform-browser": "~14.1.1",
		"@angular/platform-browser-dynamic": "~14.1.1",
		"@angular/router": "~14.1.1",
		"@mikro-orm/cli": "^5.3.1",
		"@mikro-orm/core": "^5.3.1",
		"@mikro-orm/migrations": "^5.3.1",
		"@mikro-orm/nestjs": "^5.1.1",
		"@mikro-orm/postgresql": "^5.3.1",
		"@nestjs/apollo": "^10.0.22",
		"@nestjs/common": "^9.0.0",
		"@nestjs/config": "^2.2.0",
		"@nestjs/core": "^9.0.0",
		"@nestjs/graphql": "^10.0.22",
		"@nestjs/jwt": "^9.0.0",
		"@nestjs/passport": "^9.0.0",
		"@nestjs/platform-express": "^9.0.0",
		"@nestjs/throttler": "^3.0.0",
		"@nrwl/angular": "14.5.8",
		"apollo-server-express": "^3.10.1",
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"graphql": "^16.6.0",
		"graphql-subscriptions": "^2.0.0",
		"joi": "^17.6.0",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "~7.5.0",
		"ts-morph": "^15.1.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~14.1.1",
		"@angular-eslint/eslint-plugin": "~14.0.0",
		"@angular-eslint/eslint-plugin-template": "~14.0.0",
		"@angular-eslint/template-parser": "~14.0.0",
		"@angular/cli": "~14.1.0",
		"@angular/compiler-cli": "~14.1.1",
		"@angular/language-service": "~14.1.1",
		"@nestjs/schematics": "^9.0.0",
		"@nestjs/testing": "^9.0.0",
		"@nrwl/cli": "14.5.8",
		"@nrwl/cypress": "14.5.8",
		"@nrwl/eslint-plugin-nx": "14.5.8",
		"@nrwl/jest": "14.5.8",
