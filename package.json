		"@mikro-orm/postgresql": "^5.6.8",
		"@mikro-orm/seeder": "^5.6.8",
		"@nestjs/apollo": "^10.1.7",
		"@nestjs/common": "9.4.2",
		"@nestjs/config": "^2.3.0",
		"@nestjs/core": "9.4.2",
		"@nestjs/graphql": "^10.1.7",
		"@nestjs/jwt": "^10.0.1",
		"@nestjs/passport": "^9.0.1",
		"@nestjs/platform-express": "9.4.2",
		"@nestjs/throttler": "^4.0.0",
		"@nx/angular": "16.3.2",
