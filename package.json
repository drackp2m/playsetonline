		"ts-node": "10.9.1",
		"typescript": "~4.7.2"
	},
	"mikro-orm": {
		"useTsNode": true,
		"tsConfigPath": "./apps/api/tsconfig.app.json",
		"configPaths": [
			"./apps/api/src/config/mikro-orm.config.ts"
		]
	}
}
